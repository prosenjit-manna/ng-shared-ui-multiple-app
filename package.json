{
  "name": "ng-test-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:all": "concurrently \"ng serve dashboard --port=4200\"  \"ng serve profile --port=4201\"",
    "build:all": "ng build dashboard && ng build profile",
    "serve:all": "serve -s dist/dashboard/browser -l 4200 & serve -s dist/profile/browser -l 4201",
    "transfer": "node transfer-assets.js",
    "build:transfer": "npm run build:all && npm run transfer"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.0",
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8",
    "serve": "^14.2.5"
  }
}
